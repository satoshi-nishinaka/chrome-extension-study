{
    "manifest_version": 2,
    "name": "Save Title and URL",
    "permissions": [
        "tabs",
        "clipboardWrite"
    ],
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "version": "0.0.1",
    "background": {
        "scripts": ["background.js"]
    },
    "commands": {
        "save_url_and_title": {
            "suggested_key": {
                "default": "Alt+C"
            },
            "description": "Save Title and URL"
        }
    }
}