{
  "manifest_version": 2,
  "name": "various short cuts",
  "permissions": [
    "tabs",
    "clipboardWrite",
    "storage"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "version": "0.1.0",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "",
    "default_icon": "icons/128.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "save_url_and_title": {
      "suggested_key": {
        "default": "Alt+C"
      },
      "description": "Save Title and URL"
    },
    "save_url_and_title_for_markdown": {
      "suggested_key": {
        "default": "Alt+Shift+C"
      },
      "description": "Save Title and URL for Markdown"
    },
    "save_url_and_title_and_favicon": {
      "suggested_key": {
        "default": "Alt+Shift+X"
      },
      "description": "Save Title and URL and favicon"
    }
  }
}